{
  "cli": {
    "version": ">= 16.15.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "env": {
        "NODE_ENV": "production"
      },
      "android": {
        "image": "latest",
        "node": "22.11.0",
        "env": {
          "NODE_ENV": "production"
        }
      }
    }
  },
  "submit": {
    "production": {}
  }
}
